#ifndef PLAYER_H_
#define PLAYER_H_

struct player
{
	int* x_loc;
	int* y_loc;
	int motion_x;
	int motion_y;
	int* state;
	int* missile_x;
	int* missile_y;
	int* collision;
	int* left;
	int* right;
};



#endif /* PLAYER_H_ */
